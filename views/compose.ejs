<%- include("partials/header") -%>

<h1>Compose</h1>

<form action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" class="form-control" name="composeTitle">

    <label>Post</label>
    <textarea class="form-control" rows="4" name="composeContent"></textarea>
